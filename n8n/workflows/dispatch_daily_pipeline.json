  "connections": {
    "Manual Trigger": {
      "main": [
        [
          { "node": "Dispatch Workflow", "type": "main", "index": 0 }
        ]
      ]
    },
    "Dispatch Workflow": {
      "main": [
        [
          { "node": "Slack Notify", "type": "main", "index": 0 }
        ]
      ]
    }
  }
