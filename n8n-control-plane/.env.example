# Database
POSTGRES_USER=n8n
POSTGRES_PASSWORD=change-me-strong
POSTGRES_DB=n8n

# n8n basic auth (required)
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=change-me-strong

# n8n encryption key (set a long random string)
N8N_ENCRYPTION_KEY=change-me-long-random-string

# Timezone
TZ=Europe/Rome

# Public base URL for webhooks (replace with your domain)
N8N_PROTOCOL=https
N8N_HOST=n8n.example.com
WEBHOOK_URL=https://n8n.example.com/
